<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
		{% block stylesheets %}
			{% for file in encore_entry_css_files('NBKStyle') %}
				<link rel="stylesheet" href="{{ asset(file) }}">
			{% endfor %}
		{% endblock %}
		<title>NBK</title>
	</head>

	<body class="NBK">
		<div id="nbk" data-data="{{ parameters|json_encode }}"></div>
	</body>

	{% block javascripts %}
		{% for file in encore_entry_js_files('NBK') %}
			<script src="{{ asset(file) }}"></script>
		{% endfor %}
	{% endblock %}
</html>
